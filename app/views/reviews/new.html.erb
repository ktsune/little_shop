<h2>New Review</h2>
<%= form_tag "/items/#{@item.id}" do %>
  <%= label_tag :title %>:<br>
  <%= text_field_tag :title %><br>

  <%= label_tag :content %>:<br>
  <%= text_field_tag :content %><br>

  <%= label_tag :rating %>:<br>
  <%= number_field_tag :rating %><br>

  <%= submit_tag 'Create Review' %>
<% end %>
